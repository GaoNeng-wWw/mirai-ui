{
    "compilerOptions": {
      "target": "esnext",
      "module": "Node16",
      "moduleResolution": "Node16",
      "strict": true,
      "jsx": "preserve",
      "sourceMap": true,
      "lib": ["esnext", "dom","DOM.Iterable"],
      "esModuleInterop": true,
      "declaration": true,
      "resolveJsonModule": true
    },
    "include": ["**/*.ts", "**/*.vue", ".vitepress/**/*.ts", ".vitepress/**/*.vue", ".vitepress/**/*.mts"],
    "exclude": ["node_modules"]
}
  